  },
  "devDependencies": {
    "@commitlint/cli": "18.4.0",
    "@commitlint/config-angular": "18.4.0",
    "eslint": "8.x",
    "eslint-config-nodebb": "0.2.1",
    "eslint-plugin-import": "2.x",
    "husky": "8.0.3",
    "lint-staged": "15.0.2"
  }
